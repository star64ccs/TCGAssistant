# 前端環境配置
# 複製此文件為 .env 並根據需要修改

# 環境設置
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=1.0.0

# API配置
REACT_APP_API_URL=http://localhost:3000
REACT_APP_FEATURE_REAL_API=true
REACT_APP_FEATURE_MOCK_FALLBACK=true
REACT_APP_FEATURE_NOTIFICATIONS=true
REACT_APP_FEATURE_CACHE=true

# 卡牌辨識API密鑰
REACT_APP_GOOGLE_VISION_API_KEY=your_google_vision_api_key
REACT_APP_AWS_ACCESS_KEY_ID=your_aws_access_key_id
REACT_APP_AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
REACT_APP_AWS_REGION=us-east-1
REACT_APP_AZURE_VISION_API_KEY=your_azure_vision_api_key
REACT_APP_CUSTOM_AI_API_KEY=your_custom_ai_api_key

# 價格查詢API密鑰
REACT_APP_TCGPLAYER_API_KEY=your_tcgplayer_api_key
REACT_APP_TCGPLAYER_PUBLIC_KEY=your_tcgplayer_public_key
REACT_APP_TCGPLAYER_PRIVATE_KEY=your_tcgplayer_private_key
REACT_APP_EBAY_APP_ID=your_ebay_app_id
REACT_APP_EBAY_CERT_ID=your_ebay_cert_id
REACT_APP_EBAY_CLIENT_SECRET=your_ebay_client_secret
REACT_APP_CARDMARKET_APP_TOKEN=your_cardmarket_app_token
REACT_APP_CARDMARKET_APP_SECRET=your_cardmarket_app_secret
REACT_APP_CARDMARKET_ACCESS_TOKEN=your_cardmarket_access_token
REACT_APP_CARDMARKET_ACCESS_TOKEN_SECRET=your_cardmarket_access_token_secret
REACT_APP_PRICECHARTING_API_KEY=your_pricecharting_api_key
REACT_APP_MERCARI_API_KEY=your_mercari_api_key
REACT_APP_SNKRDUNK_API_KEY=your_snkrdunk_api_key

# AI分析API密鑰
REACT_APP_OPENAI_API_KEY=your_openai_api_key
REACT_APP_GOOGLE_PALM_API_KEY=your_google_palm_api_key
REACT_APP_AZURE_OPENAI_API_KEY=your_azure_openai_api_key

# 性能配置
REACT_APP_CACHE_DURATION=300000
REACT_APP_MAX_CACHE_SIZE=50
REACT_APP_MAX_RETRIES=3
REACT_APP_RETRY_DELAY=1000
REACT_APP_API_TIMEOUT=30000
REACT_APP_UPLOAD_TIMEOUT=60000

# 圖片處理配置
REACT_APP_MAX_IMAGE_SIZE=10485760
REACT_APP_MAX_IMAGE_WIDTH=1920
REACT_APP_MAX_IMAGE_HEIGHT=1080
REACT_APP_IMAGE_QUALITY=0.9

# 會員限制配置
REACT_APP_FREE_DAILY_LIMIT=5
REACT_APP_VIP_DAILY_LIMIT=50
REACT_APP_PREMIUM_DAILY_LIMIT=1000

# 安全配置
REACT_APP_ENABLE_SSL_PINNING=false
REACT_APP_ENABLE_CERT_PINNING=false

# 開發工具配置
REACT_APP_ENABLE_LOGGING=true
REACT_APP_LOG_LEVEL=info
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_DEBUG=false

# 測試配置
REACT_APP_ENABLE_TEST_MODE=false
REACT_APP_TEST_API_URL=http://localhost:3000/api/v1
