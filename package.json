{
  "name": "tcg-assistant",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "start:optimized": "expo start --optimize",
    "start:clear": "expo start --clear",
    "start:tunnel": "expo start --tunnel",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:src": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:src:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "lint:src:check": "eslint src/ --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:report": "eslint . --ext .js,.jsx,.ts,.tsx --format=json --output-file=eslint-report.json",
    "format": "node scripts/format-code.js",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "style:check": "npm run lint:check && npm run format:check",
    "style:fix": "npm run format:write && npm run lint:fix",
    "type-check": "echo 'TypeScript check skipped for JavaScript project'",
    "clean": "rm -rf node_modules && npm install",
    "clean:cache": "expo r -c && npm start -- --clear",
    "build:analyze": "expo export --platform web",
    "prebuild": "npm run style:check && npm run type-check",
    "postinstall": "expo install --fix",
    "expo": "expo start",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:web": "expo start --web"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native/metro-config": "^0.76.8",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.2",
    "expo": "~53.0.0",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-localization": "~16.1.6",
    "i18next": "^23.7.6",
    "metro": "^0.82.5",
    "metro-cache": "^0.76.8",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^13.5.0",
    "react-native": "0.79.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localize": "^3.5.1",
    "react-native-qrcode-svg": "^6.3.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-share": "^9.4.1",
    "react-native-svg": "15.11.2",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@expo/metro-config": "^0.20.17",
    "@react-native/eslint-config": "^0.72.2",
    "@testing-library/react-native": "^13.2.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/node": "^24.2.1",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "depcheck": "^1.4.7",
    "eslint": "^8.19.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^30.0.5",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "redux-mock-store": "^1.5.5",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": ">=16"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "redux-persist"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
